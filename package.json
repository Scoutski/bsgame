    "sequelize": "^4.26.0",
    "uuid": "^3.1.0",
    "vue": "^2.5.10",
    "vue-loader": "^13.5.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.10",
    "vueify": "^9.4.1"
  },
